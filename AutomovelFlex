package application;

public class AutomovelFlex extends Automovel{

	private String marca;
	private String modelo;
	private int anoDeFabricacao;
	protected String combustivel2;
	
	public AutomovelFlex(String marca, String modelo, int anoDeFabricacao,
			int anoDeModelo, float motorizacao, String combustivel1,String combustivel2,
			String cor, String numPlaca, int numRenavam) {
		super(marca, modelo, anoDeFabricacao, anoDeModelo, motorizacao, combustivel1,
				cor, numPlaca, numRenavam);
		
		this.marca = marca;
		this.modelo = modelo;
		this.anoDeFabricacao = anoDeFabricacao;
		this.anoDeModelo = anoDeModelo;
		this.motorizacao = motorizacao;
		this.combustivel1 = combustivel1;
		this.cor = cor;
		this.numPlaca = numPlaca;
		this.numRenavam = numRenavam;
		this.combustivel2 = combustivel2;
		
	}

	public String getCombustivel2() {
		return combustivel2;
	}

	public void setCombustivel2(String combustivel2) {
		this.combustivel2 = combustivel2;
	}

	

}
