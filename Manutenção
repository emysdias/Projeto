
package entities;

import javax.swing.JOptionPane;



public class Manutencao extends Despesas {

	private float quilometragem;
	private double totalValue;
	private String type;
	
	public Manutencao(double totalValue, String type, float quilometragem) {
		
		this.quilometragem = quilometragem;
		this.totalValue = totalValue;
		this.type = type;
	}

	@Override
	public String toString() {
		return "\nManutencao:\n Nome Da despesa = " + type + "\nTotal pago = "
				+ totalValue + "\nQuilometragem = " + quilometragem + "";
	}

	

}
