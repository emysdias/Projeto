package entities;









public class Abastecimento extends Despesas {



	private float anterior;
	private float atual;
	private String data;
	private String tipoDeCombustivel;
	private float quilometragem;
	private float valorDoLitro;
	private double valorAbastecido;


	public Abastecimento(String data,String tipoDeCombustivel, float quilometragem, float valorDoLitro,
			double valorAbastecido, int tanque) {
		
		
		this.atual = atual;
		this.data = data;
		this.tipoDeCombustivel = tipoDeCombustivel;
		this.quilometragem = quilometragem;
		this.valorDoLitro = valorDoLitro;
		this.valorAbastecido = valorAbastecido;

	}



	public double Abastecimento(float valorDoLitro, float litrosAbastecidos  ) {

		this.valorAbastecido = valorDoLitro * litrosAbastecidos; 
		return valorDoLitro * litrosAbastecidos;

	}



	@Override
	public String toString() {
		return "\nAbastecimento:\nOdômetro anterior = " + anterior + "\nOdômetro atual = " + quilometragem
				+ "\nData = " + data + "\nTipo de combustível = " + tipoDeCombustivel
				+ "\nValor total do abastecimento = "
				+ valorDoLitro
				+ "\nQuantidade abastecida em litros = " + valorAbastecido + "";
	}
	
	


}
